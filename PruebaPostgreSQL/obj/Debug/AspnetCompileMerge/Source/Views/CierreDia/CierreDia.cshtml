
@model Negocio.Cierres.CierreDiaViewModel
@{
    ViewBag.Title = "Cierre Dia";
}

@using (Html.BeginForm())
{
    
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h1 class="title">Cierre de Cartera</h1>
        </div>
        <div class="panel-body">
            <h6>Fecha de cierre @Model.Fecha.ToShortDateString()</h6>
        </div>
        <div class="panel-footer">
            <button class="btn btn-primary" type="submit">correr cierre</button>
        </div>
    </div>
}